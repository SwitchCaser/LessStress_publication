<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dip" />
            <gradient
                android:startColor="#DA9CB1"
                android:centerColor="#FFDAE6"
                android:centerY="0.75"
                android:endColor="#EDAEC3"
                android:angle="180"
                />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip android:clipOrientation="vertical" android:gravity="bottom">
            <shape>
                <corners android:radius="5dip" />
                <gradient
                    android:startColor="#92F8AFC8"
                    android:centerColor="#86EF79A1"
                    android:centerY="0.75"
                    android:endColor="#8AD685A0"
                    android:angle="180"
                    />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip android:clipOrientation="vertical" android:gravity="bottom">
            <shape>
                <corners android:radius="5dip" />
                <gradient
                    android:startColor="#F46193"
                    android:centerColor="#E91E63"
                    android:centerY="0.75"
                    android:endColor="#D53E71"
                    android:angle="180"
                    />
            </shape>
        </clip>
    </item>
</layer-list>
<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Background"
    tools:context=".Store">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/lay1"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@color/HappyGreen">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="@font/fontone"
            android:gravity="center"
            android:text="Магазин"
            android:textColor="@color/Green"
            android:textSize="30dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/lay2"
        android:layout_width="match_parent"
        android:layout_height="135dp"
        app:layout_constraintTop_toBottomOf="@+id/lay1"
        tools:layout_editor_absoluteX="16dp"
        android:background="#D4E6B0">

        <TextView
            android:id="@+id/textView8"
            android:layout_width="55dp"
            android:layout_height="match_parent"
            android:fontFamily="@font/fontone"
            android:gravity="center"
            android:rotation="-90"
            android:text="Коврики"
            android:textColor="@color/Green"
            android:textSize="13dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/carpet1"
            android:layout_width="118dp"
            android:layout_height="match_parent"
            android:backgroundTint="@color/caldroid_transparent"
            android:rotation="90"
            android:scaleType="fitCenter"
            android:src="@drawable/carp2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/carpet2"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <ImageButton
            android:id="@+id/carpet2"
            android:layout_width="118dp"
            android:layout_height="match_parent"
            android:backgroundTint="@color/caldroid_transparent"
            android:rotation="90"
            android:scaleType="fitCenter"
            android:src="@drawable/carp1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView8"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <ImageButton
            android:id="@+id/carpet3"
            android:layout_width="118dp"
            android:layout_height="match_parent"
            android:backgroundTint="@color/caldroid_transparent"
            android:rotation="90"
            android:scaleType="fitCenter"
            android:src="@drawable/carp3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/carpet1"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/lay3"
        android:layout_width="match_parent"
        android:layout_height="135dp"
        app:layout_constraintTop_toBottomOf="@+id/lay2"
        tools:layout_editor_absoluteX="16dp"
        android:background="@color/HappyGreen">

        <TextView
            android:id="@+id/textView9"
            android:layout_width="55dp"
            android:layout_height="match_parent"
            android:fontFamily="@font/fontone"
            android:gravity="center"
            android:rotation="-90"
            android:text="Растения"
            android:textColor="@color/Green"
            android:textSize="11dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/plant1"
            android:layout_width="119dp"
            android:layout_height="match_parent"
            android:backgroundTint="@color/caldroid_transparent"
            android:scaleType="fitCenter"
            android:src="@drawable/plant1"
            app:layout_constraintStart_toEndOf="@+id/plant2"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <ImageButton
            android:id="@+id/plant2"
            android:layout_width="119dp"
            android:layout_height="match_parent"
            android:backgroundTint="@color/caldroid_transparent"
            android:scaleType="fitCenter"
            android:src="@drawable/plant2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView9"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            tools:ignore="MissingConstraints" />

        <ImageButton
            android:id="@+id/plant3"
            android:layout_width="118dp"
            android:layout_height="match_parent"
            android:backgroundTint="@color/caldroid_transparent"
            android:scaleType="fitCenter"
            android:src="@drawable/plant3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/plant1"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/lay4"
        android:layout_width="match_parent"
        android:layout_height="135dp"
        app:layout_constraintTop_toBottomOf="@+id/lay3"
        tools:layout_editor_absoluteX="16dp"
        android:background="#D4E6B0">

        <TextView
            android:id="@+id/textView10"
            android:layout_width="55dp"
            android:layout_height="match_parent"
            android:fontFamily="@font/fontone"
            android:gravity="center"
            android:rotation="-90"
            android:text="Обои"
            android:textColor="@color/Green"
            android:textSize="13dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/wall1"
            android:layout_width="119dp"
            android:layout_height="match_parent"
            android:backgroundTint="@color/caldroid_transparent"
            android:scaleType="fitCenter"
            android:src="@drawable/wall1"
            app:layout_constraintStart_toEndOf="@+id/textView10"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <ImageButton
            android:id="@+id/wall2"
            android:layout_width="119dp"
            android:layout_height="match_parent"
            android:backgroundTint="@color/caldroid_transparent"
            android:scaleType="fitCenter"
            android:src="@drawable/wall2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/wall1"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            tools:ignore="MissingConstraints" />

        <ImageButton
            android:id="@+id/wall3"
            android:layout_width="118dp"
            android:layout_height="match_parent"
            android:backgroundTint="@color/caldroid_transparent"
            android:scaleType="fitCenter"
            android:src="@drawable/wall3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/wall2"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/lay5"
        android:layout_width="match_parent"
        android:layout_height="135dp"
        app:layout_constraintTop_toBottomOf="@+id/lay4"
        tools:layout_editor_absoluteX="16dp"
        android:background="@color/HappyGreen">

        <TextView
            android:id="@+id/textView11"
            android:layout_width="55dp"
            android:layout_height="match_parent"
            android:fontFamily="@font/fontone"
            android:gravity="center"
            android:rotation="-90"
            android:text="Кровати"
            android:textColor="@color/Green"
            android:textSize="13dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/bed1"
            android:layout_width="119dp"
            android:layout_height="match_parent"
            android:backgroundTint="@color/caldroid_transparent"
            android:scaleType="fitCenter"
            android:src="@drawable/bed1"
            app:layout_constraintStart_toEndOf="@+id/textView11"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <ImageButton
            android:id="@+id/bed2"
            android:layout_width="119dp"
            android:layout_height="match_parent"
            android:backgroundTint="@color/caldroid_transparent"
            android:scaleType="fitCenter"
            android:src="@drawable/bed2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/bed1"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            tools:ignore="MissingConstraints" />

        <ImageButton
            android:id="@+id/bed3"
            android:layout_width="118dp"
            android:layout_height="match_parent"
            android:backgroundTint="@color/caldroid_transparent"
            android:scaleType="fitCenter"
            android:src="@drawable/bed3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/bed2"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/lay6"
        android:layout_width="match_parent"
        android:layout_height="135dp"
        app:layout_constraintTop_toBottomOf="@+id/lay5"
        tools:layout_editor_absoluteX="16dp"
        android:background="#D4E6B0">

        <TextView
            android:id="@+id/textView12"
            android:layout_width="55dp"
            android:layout_height="match_parent"
            android:fontFamily="@font/fontone"
            android:gravity="center"
            android:rotation="-90"
            android:text="Окна"
            android:textColor="@color/Green"
            android:textSize="13dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/w1"
            android:layout_width="119dp"
            android:layout_height="match_parent"
            android:backgroundTint="@color/caldroid_transparent"
            android:scaleType="fitCenter"
            android:src="@drawable/w1_day"
            app:layout_constraintStart_toEndOf="@+id/textView12"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <ImageButton
            android:id="@+id/w2"
            android:layout_width="119dp"
            android:layout_height="match_parent"
            android:backgroundTint="@color/caldroid_transparent"
            android:scaleType="fitCenter"
            android:src="@drawable/w2_day"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/w1"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            tools:ignore="MissingConstraints" />

        <ImageButton
            android:id="@+id/w3"
            android:layout_width="118dp"
            android:layout_height="match_parent"
            android:backgroundTint="@color/caldroid_transparent"
            android:scaleType="fitCenter"
            android:src="@drawable/w3_day"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/w2"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>